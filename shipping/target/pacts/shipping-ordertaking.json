{
  "consumer": {
    "name": "shipping"
  },
  "provider": {
    "name": "ordertaking"
  },
  "messages": [
    {
      "description": "order placed message",
      "metaData": {
        "contentType": "application/json"
      },
      "contents": {
        "orderLines": [
          {
            "productCode": "product-code",
            "quantity": 10
          }
        ],
        "orderId": "order-id",
        "shippingAddress": "a shipping address"
      },
      "providerStates": [
        {
          "name": "an order with id order-id has been placed"
        }
      ],
      "matchingRules": {
        "body": {
          "$.orderLines[0].productCode": {
            "matchers": [
              {
                "match": "type"
              }
            ],
            "combine": "AND"
          },
          "$.orderLines[0].quantity": {
            "matchers": [
              {
                "match": "number"
              }
            ],
            "combine": "AND"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.10"
    }
  }
}